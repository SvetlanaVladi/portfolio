%matplotlib inline
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
from sklearn.linear_model import LinearRegression
import scipy.stats as st
dataset = pd.read_csv('https://video.ittensive.com/math-stat/data-6048-2020-06-29.utf.csv', ';')
dataset.head()

dataset.plot(x='Quarter', y='AveragePerCapita', style='o')
plt.show()

sen_=dataset['Seniors']
chi_=dataset['Children']
stats = st.f_oneway(sen_, chi_)
print(stats)
if stats[1]<0.5:
    if st.ttest_ind(sen_, chi_)[1]<0.05/2:
        print("различия есть")
else:
    print("Различий нет")F_onewayResult(statistic=9.407847294851887, pvalue=0.0027542901076847634) различия есть

dataset.describe()
X=dataset[['WorkingPopulation', 'Seniors', 'Children']] # расчет со столбцом Quarter не получался, но суть я поняла
y=dataset['AveragePerCapita']
from sklearn.model_selection import train_test_split
X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, random_state=0)
from sklearn.linear_model import LinearRegression
regressor = LinearRegression()
regressor.fit(X_train, y_train)
coeff_df = pd.DataFrame(regressor.coef_, X.columns, columns=['Coefficient'])
coeff_df


y_pred = regressor.predict(X_test)
df = pd.DataFrame({'Actual': y_test, 'Predicted': y_pred})
df
